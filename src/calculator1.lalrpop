use std::str::{ FromStr, };

grammar;

Num: i32 =
    <s:r"[0-9]+"> => i32::from_str(s).unwrap();

pub Term: i32 = {
    <n:Num> => n,
    "(" <t:Term> ")" => t,
};

extern {
    type Location = usize;
    type Error = lexer::LexicalError;

    enum lexer::Tok {
        Symbol(String),
        Float(f64),
        Int(i64),
        LParen,
        RParen,
        Dot,
        Space,
    }
}
